<!doctype html>

<html class="home blog no-js" lang="en-US">
<head>
    <title>Kaaf Events</title>

    <meta charset="UTF-8"/>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" type="text/css" media="all"/>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Amatic+SC%3A400%2C700%7CLato%3A400%2C700%2C400italic%2C700italic&amp;ver=4.9.8"
          type="text/css" media="screen"/>
    <link rel="stylesheet" href="/public/user/css/style.css" type="text/css" media="screen"/>
    <link rel="stylesheet" href="/public/user/css/print.css" type="text/css" media="print"/>
    <link rel="icon" href="/public/user/images/kaaflogo.png">



    <script src='/public/user/js/jquery-3.0.0.min.js'></script>
    <script src='/public/user/js/jquery-migrate-3.0.1.min.js'></script>
  

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">

<style>
    .text-black{
        color: black;
        font-size:2rem;
    }
    .text-bl{
        color: black;
        font-size:1.5rem;

    }
</style>


</head>
<body class="home sticky-menu sidebar-off" id="top">


 <!-- Header  -->
 <%- include('header') -%> 
 <!-- Header end  -->
 <div class="inner">

  <h2 class="entry-title"></h2>
  </div>
 <div class="card">
    <div class="card-body"><center><h1 class="fw-semibold mb-4">Bookings</h1></center>
      <br><br>
      <!-- table begin -->
<div class="card w-100">
<div class="card-body p-3">

<div class="table-responsive">
<table class="table text-nowrap mb-0 align-middle">
  <thead class="text-black fs-4">
    <tr>
      <th class="border-bottom-0">
        <h6 class="fw-semibold mb-0 text-black">Sl no</h6>
      </th>
      <th class="border-bottom-0 text-black">
        <h6 class="fw-semibold mb-0 text-black">Customer name</h6>
      </th>
      <th class="border-bottom-0">
        <h6 class="fw-semibold mb-0 text-black">Description</h6>
      </th>
      <th class="border-bottom-0">
        <h6 class="fw-semibold mb-0 text-black">Status</h6>
      </th>
      <th class="border-bottom-0">
        <h6 class="fw-semibold mb-0 text-black">Advance Amount</h6>
      </th>
      <th class="border-bottom-0" colspan="2">
        <h6 class="fw-semibold mb-0 text-black">Action </h6>
      </th>
    </tr>
  </thead>
  <tbody>
    
    <% if ( bookings && bookings.length > 0) { %>
      <% bookings.forEach((Items, index) => { %>  
                <tr>
                    <td class="border-bottom-0"><h6 class="fw-semibold mb-0"><%= index + 1 %></h6></td>
                    <td class="border-bottom-0">
                      <p class="mb-0 fw-large text-bl"><%=Items.username%></p>                         
                    </td>
                    <td class="border-bottom-0">
                      <p class="mb-0 fw-normal text-bl"><%=Items.eventType%> on <%=Items.eventMode%> at <%=Items.eventdate%></p>
                    </td>
                    <td class="border-bottom-0">
                      <p class="mb-0 fw-normal text-bl"><%=Items.status%></p>
                    </td>
                    <td class="border-bottom-0">
                      <p class="mb-0 fw-normal text-bl">2000</p>
                    </td>
                    <td class="border-bottom-0">
                      <span><a href="/user/delete-status/<%=Items.booking_id%>"><i class="ti ti-trash btn btn-danger">cancel</i></a></span>
                    </td>
                  </tr> 
                  <% }); %>
                      <% } else { %>
                        <tr>
                          <td colspan="5" class="text-center">No data available</td>
                        </tr>
                      <% } %>   
               
  </tbody>
</table>
</div>
</div>
</div><br><br><br>